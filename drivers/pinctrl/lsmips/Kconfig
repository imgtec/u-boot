
config PINCTRL_LSMIPS
	depends on ARCH_LSMIPS
	bool

config PINCTRL_LS1B200
	bool "Loongson LS1B200 pin control driver"
	select PINCTRL_LSMIPS
	depends on SOC_LS1B200 && PINCTRL_GENERIC
	help
	  Support pin multiplexing control on MediaTek LS7628.
	  The driver is controlled by a device tree node which contains
	  the pin mux functions for each available pin groups.

